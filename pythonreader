import glob, os
os.chdir("/databases/mol/pdb/cg")
import re

list1 = []
list2 = []
def stringfinder(filename, string):
 with open(filename, 'r') as f:
  for line in f:
   if string in line:
    return True
 return False

for file in glob.glob("*.ent"):
 if stringfinder(file, 'ZN'):
  list1.append(file)

